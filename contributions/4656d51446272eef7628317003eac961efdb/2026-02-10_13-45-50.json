      "solution": "import java.util.*;\nimport java.io.*;\nimport java.math.*;\n\npublic class Solution {\n    public static void main(String[] args){\n        Scanner in = new Scanner(System.in);\n\n        int r = in.nextInt();\n        int n=0;\n        for(int i=0; i<r; i++){\n            for(int j=0; j<=i; j++){\n                if(j == i){\n                 System.out.print(++n);\n                 continue;\n                }\n                System.out.print(++n+\" \");\n            }\n            System.out.println();\n        }\n    }\n}",
