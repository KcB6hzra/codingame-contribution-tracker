      "solution": "import java.util.*\nimport java.io.*\nimport java.math.*\n\ntypealias Gear = Triple<Int,Int,Int>\nfun main(args : Array<String>) {\n    \n    val input = Scanner(System.`in`)\n    val N = input.nextInt()\n    var gears = listOf<Gear>()\n    for (i in 0 until N) {\n        val x = input.nextInt()\n        val y = input.nextInt()\n        val r = input.nextInt()\n        \n        gears += Triple(x, y, r)\n    }\n    \n    fun Gear.connectedTo(other:Gear):Boolean{\n       if(this == other) return false\n       \n       val dx = first - other.first\n       val dy =  second - other.second\n       val dist = third + other.third\n       \n       val abSquared = dx*dx + dy*dy\n       val cSquared = dist * dist\n       \n       // Debuggin test cases:\n       check(abSquared >= cSquared){\"Gears should not overlap ($this, $other)\"}\n\n       return abSquared == cSquared\n    }\n    \n    val last = gears.last()\n    val queue = mutableListOf(gears.first())\n    val directions = mutableMapOf(gears.first() to true)\n    while(!queue.isEmpty()){\n        val gear = queue.removeAt(0)\n        val nextDirection = ! directions[gear]!!\n        \n        gears.filter{ gear.connectedTo(it) }\n            .forEach{\n                val direction = directions[it]\n//                System.err.println(\"$it $direction\")\n                if(direction == null){\n                    directions[it] = nextDirection\n                    queue.add(it)\n//                    System.err.println(\"Added $it $nextDirection\")\n                }else if(direction != nextDirection){\n                    println(\"NOT MOVING\")\n                    return\n                }\n            }\n    }\n\n    val direction = when(directions[last]){\n        true -> \"CW\"\n        false -> \"CCW\"\n        else -> \"NOT MOVING\"\n    }\n    println(direction)\n}",
