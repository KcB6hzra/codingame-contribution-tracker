      "shortest": true,
      "solution": "using System;\nusing System.Linq;\nusing System.Collections.Generic;\n\nclass Solution\n{\n    static void Main(string[] args)\n    {\n        int stayInformationCount = int.Parse(Console.ReadLine());\n        var dictionary = new Dictionary<DateTime, int>();\n        for (int i = 0; i < stayInformationCount; i++)\n        {\n            string[] inputs = Console.ReadLine().Split(' ');\n            var arrival = DateTime.Parse(inputs[0]);\n            var departure = DateTime.Parse(inputs[1]);\n            int roomCount = int.Parse(inputs[2]);\n\n            var day = arrival;\n            while(day < departure)\n            {\n                dictionary.TryGetValue(day, out int existingCount);\n                dictionary[day] = roomCount + existingCount;\n                day = day.AddDays(1);\n            }\n        }\n        Console.WriteLine(dictionary.Values.Max());\n    }\n}",
