      "solution": "using System;\nusing System.Linq;\nusing System.IO;\nusing System.Text;\nusing System.Collections;\nusing System.Collections.Generic;\n\nclass BingoCard\n{\n    private int id;\n    private int[,] numbers;\n\n    public BingoCard() {\n        numbers = new int[5, 5];\n    }\n    public void SetLine(int line, string[] lineNumbers) {\n        for(int i = 0; i < 5; i++) {\n            numbers[line, i] = int.Parse(lineNumbers[i]);\n        }\n    }\n    public void AddNumber(int num) {\n        if (!(Solution.CheckBingos))\n            return;\n        \n        // If we have a match, assign \"0\" in its place\n        for(var x = 0; x < 5; x++) {\n            for(var y = 0; y < 5; y++) {\n                if (numbers[x, y] == num)\n                    numbers[x, y] = 0;\n            }\n        }\n\n        // Check what we are searching for (avoid excessive searching)\n        if (Solution.CheckLines)\n            CheckLine();\n        else\n            CheckBingo();\n    }\n    // Check for bingo\n    private void CheckBingo() {\n        bool found = true;\n        for(var x = 0; x < 5 && found; x++) {\n            for(var y = 0; y < 5 && found; y++) {\n                if (!(numbers[x, y] == 0))\n                    found = false;\n            }\n        }\n        if (found)\n            Solution.SetBingo();\n    }\n    // Check for a line\n    private void CheckLine() {\n        bool found = false;\n        for (var i = 0; i < 5 && !(found); i++) {\n            // Check row\n                found = true;\n                var thisRow = Row(i);\n                for (var x = 0; x < 5 && found; x++) {\n                    if (!(thisRow[x] == 0))\n                        found = false;\n            }\n            // Check column\n            if (!(found)) {\n                found = true;\n                var thisCol = Column(i);\n                for(var y = 0; y < 5 && found; y++) {\n                    if (!(thisCol[y] == 0))\n                        found = false;\n                }\n            }\n        }\n        // Check first diagonal\n        if (!(found)) {\n            found = true;\n            var thisDiag = Diagonal(0);\n            for (var x = 0; x < 5 && found; x++) {\n                if (!(thisDiag[x] == 0))\n                    found = false;\n            }\n        }\n        // Check second diagonal\n        if (!(found)) {\n            found = true;\n            var thisDiag = Diagonal(1);\n            for (var x = 0; x < 5 && found; x++) {\n                if (!(thisDiag[x] == 0))\n                    found = false;\n            }\n        }\n        if (found)\n            Solution.SetLine();\n    }\n    // Return a row\n    private int[] Row(int index) {\n        int[] rtnRow = new int[5];\n        for (var i = 0; i < 5; i++)\n        rtnRow[i] = numbers[index, i];\n        return rtnRow;\n    }\n    // Return a column\n    private int[] Column(int index) {\n        int[] rtnColumn = new int[5];\n        for (var i = 0; i < 5; i++)\n        rtnColumn[i] = numbers[i, index];\n        return rtnColumn;\n    }\n    // Return a diagonal (index 0 == 0,0 1,1 etc. index 1 == 0,4 1,3 etc.)\n    private int[] Diagonal(int index) {\n        int[] rtnDiagonal = new int[5];\n        if (index == 0) {\n            for(var i = 0; i < 5; i++)\n                rtnDiagonal[i] = numbers[i, i];\n        } else {\n            rtnDiagonal[0] = numbers[0, 4];\n            rtnDiagonal[1] = numbers[1, 3];\n            rtnDiagonal[2] = numbers[2, 2];\n            rtnDiagonal[3] = numbers[3, 1];\n            rtnDiagonal[4] = numbers[4, 0];\n        }\n        return rtnDiagonal;\n    }\n}\nclass Solution\n{\n    static private int firstLine = 0;\n    static private int firstBingo = 0;\n    static private int turn = 0;\n    // Whether to check for lines\n    static public bool CheckLines {\n        get { return firstLine == 0; }\n    }\n    // Whether to check for bingos\n    static public bool CheckBingos {\n        get { return firstBingo == 0; }\n    }\n    // Set the first line found\n    public static void SetLine() {\n        if (firstLine == 0)\n            firstLine = turn;\n    }\n    // Set the first bingo\n    public static void SetBingo() {\n        if (firstBingo == 0)\n            firstBingo = turn;\n    }\n    static void Main(string[] args)\n    {\n        // Build bingo cards\n        int n = int.Parse(Console.ReadLine());\n        BingoCard[] cards = new BingoCard[n];\n        for (var i = 0; i < n; i++)\n        {\n            cards[i] = new BingoCard();\n            for (var j = 0; j < 5; j++)\n            {\n                string[] inputs = Console.ReadLine().Split(' ');\n                cards[i].SetLine(j, inputs);\n            }\n        }\n        // Go through bingo calls\n        string[] bingocalls = Console.ReadLine().Split(' ');\n        for (var i = 0; i < bingocalls.Length; i++) {\n            turn++;\n            var thisNumber = int.Parse(bingocalls[i]);\n            foreach (BingoCard card in cards)\n                card.AddNumber(thisNumber);\n        }\n        // Write the solution\n        Console.WriteLine(firstLine);\n        Console.WriteLine(firstBingo);\n    }\n}",
      "statement": "Provided with a set of <<bingo cards>>, and the order in which the numbers will be called, output how many numbers need to be called before somebody has a <<line>>, followed by how many numbers need to be called before somebody has \"bingo\" (all numbers on their sheet filled in).\n\nA <<bingo card>> is defined as a {{5}}x{{5}} set of numbers between 1 and 90. The center of a bingo card is a \"free space\", meaning it is already filled in - this is denoted with a {{0}} in this puzzle. \n\nA <<line>> on the bingo card is defined as any row, column or diagonal of 5 numbers on the card.",
