      "solution": "#include <ostream>\n#include <iostream>\n#include <vector>\n#include <algorithm>\n#include <ios>\n#include <istream>\n#include <math.h>\n#include <iomanip>\n#include <string.h>\nusing namespace std;\nint main(void){\n    ios::sync_with_stdio(0);cin.tie(0);cout.tie(0);\n    int n,has=0;cin>>n;cin.ignore();\n    string s;getline(cin,s);\n    for(int i = 0;i < s.length() - 3;i++) \n        if(i <= s.length()-5 && s.substr(i, 5)==\"clash\") s=s.substr(0,i)+\"code\"+s.substr(i+5),has=1;\n        else if(s.substr(i, 4)==\"code\") s=s.substr(0,i)+\"clash\"+s.substr(i+4),has=1;\n    cout << (has?s:\"None\") << '\\n';\n    return 0;\n}",
