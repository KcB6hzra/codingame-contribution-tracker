      "solution": "using System;\nusing System.Linq;\nusing System.IO;\nusing System.Text;\nusing System.Collections;\nusing System.Collections.Generic;\n\n/**\n * Auto-generated code below aims at helping you parse\n * the standard input according to the problem statement.\n **/\nclass Solution\n{\n    static void Main(string[] args)\n    {\n        string[] input = Console.ReadLine().Split(' ');\n\n        int h = Convert.ToInt16(input[0]);\n        int w = Convert.ToInt16(input[1]);\n        int x = Convert.ToInt16(input[2]);\n        int y = Convert.ToInt16(input[3]);\n        int d = Convert.ToInt16(input[4]);\n        double rad = 0.5 * d;\n\n        var Ray = new string[h,w];\n\n        for (int hh = 0 ; hh < h ; hh++)\n        {\n            for (int ww = 0 ; ww < w ; ww++)\n            {\n                Ray[hh,ww] = \"{}\";\n            }\n        }\n\n        for (int hh = 0 ; hh < h ; hh++)\n        {\n            for (int ww = 0 ; ww < w ; ww++)\n            {\n                int diffX = ww - x;\n                int diffY = hh - y;   \n                double temp = diffX * diffX + diffY * diffY;\n                double radiusToConsider = Math.Sqrt(temp);\n                if (radiusToConsider <= rad)\n                {\n                    Ray[hh,ww] = \"  \";\n                }\n            }\n        }\n        PrintRay(Ray);\n    }\n\n\n    public static void PrintRay<T>(T[,] matrix)\n    {\n        for (int i = 0; i < matrix.GetLength(0); i++)\n        {\n            for (int j = 0; j < matrix.GetLength(1); j++)\n            {\n                Console.Write(matrix[i,j] + \"\");\n            }\n            Console.WriteLine();\n        }\n    }\n}",
