      "solution": "import java.util.*;\r\n\r\nclass Solution {\r\n\r\n    public static void main(String args[]) {\r\n        Scanner in = new Scanner(System.in);\r\n        int n = in.nextInt();\r\n\r\n        for (int i = 0; i < n; i++) {\r\n            int start = i + 1;\r\n            for (int j = 0; j < n; j++)  {\r\n                if (j != 0) System.out.print(\" \");\r\n                System.out.print(start+j*n);\r\n            }\r\n            System.out.println();\r\n        }\r\n    }\r\n    \r\n}",
