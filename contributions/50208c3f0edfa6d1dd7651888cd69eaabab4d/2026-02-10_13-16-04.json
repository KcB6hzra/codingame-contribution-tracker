      "solution": "h, w = [int(i) for i in input().split()]\ngrille = []\nsommets = []\nmeme_ligne = {}\nm_l = 0\nfor i in range(h) :\n    line = input()\n    ligne = list(line)\n    grille.append(ligne)\n    if \"+\" in line :\n        ok = False\n        m_l += 1\n        for j in range(w) :\n            c = ligne[j]\n            if c == \"+\" :\n                ok = True\n                sommets.append((i,j))\n                meme_ligne[(i,j)] = m_l\n            elif c == \"-\" :\n                pass\n            else :\n                if ok :\n                    m_l += 1\n                ok = False\n\nmeme_colonne = {}\nm_c = 0\nfor j in range(w) :\n    ok = False\n    m_c += 1\n    for i in range(h) :\n        c = grille[i][j]\n        if c == \"+\" :\n            ok = True\n            meme_colonne[(i,j)] = m_c\n        elif c == \"|\" :\n            pass\n        else :\n            if ok :\n                m_c += 1\n            ok = False\n\nmeme_diag1 = {}\nm_d1 = 0\nfor j in range(w) :\n    ok = False\n    m_d1 += 1\n    for i in range(h) :\n        if j+i < w :\n            c = grille[i][j+i]\n            if c == \"+\" :\n                ok = True\n                meme_diag1[(i,j+i)] = m_d1\n            elif c == \"\\\\\" :\n                pass\n            else :\n                if ok :\n                    m_d1 += 1\n                ok = False\nfor i in range(1,h) :\n    ok = False\n    m_d1 += 1\n    for j in range(w) :\n        if i+j < h :\n            c = grille[i+j][j]\n            if c == \"+\" :\n                ok = True\n                meme_diag1[(i+j,j)] = m_d1\n            elif c == \"\\\\\" :\n                pass\n            else :\n                if ok :\n                    m_d1 += 1\n                ok = False\n\n\nmeme_diag2 = {}\nm_d2 = 0\nfor j in range(w) :\n    ok = False\n    m_d2 += 1\n    for i in range(h) :\n        if 0 <= j-i :\n            c = grille[i][j-i]\n            if c == \"+\" :\n                ok = True\n                meme_diag2[(i,j-i)] = m_d2\n            elif c == \"/\" :\n                pass\n            else :\n                if ok :\n                    m_d2 += 1\n                ok = False\nfor i in range(1,h) :\n    ok = False\n    m_d2 += 1\n    for j in range(w) :\n        if i+j < h :\n            c = grille[i+j][w-1-j]\n            if c == \"+\" :\n                ok = True\n                meme_diag2[(i+j,w-1-j)] = m_d2\n            elif c == \"/\" :\n                pass\n            else :\n                if ok :\n                    m_d2 += 1\n                ok = False\n\n\nnb = len(sommets)\n\nnb_rectangles = 0\nfor i in range(nb) :\n    (i1, j1) = sommets[i]\n    for j in range(i+1, nb) :\n        (i2, j2) = sommets[j]\n        if i1 < i2 and j1 < j2 and grille[i1][j2] == \"+\" == grille[i2][j1] and meme_ligne[(i1,j1)] == meme_ligne[(i1,j2)] and meme_ligne[(i2,j1)] == meme_ligne[(i2,j2)] and meme_colonne[(i1,j1)] == meme_colonne[(i2,j1)] and meme_colonne[(i1,j2)] == meme_colonne[(i2,j2)] :\n            nb_rectangles += 1\n\nnb_triangles = 0\nfor i in range(nb) :\n    (i1, j1) = sommets[i]\n    for j in range(i+1, nb) :\n            (i2, j2) = sommets[j]\n            if (i1 == i2 and j1 < j2 and meme_ligne[(i1,j1)] == meme_ligne[(i2,j2)]) :\n                if (j1 + j2)%2 == 0 :\n                    j3 = (j1 + j2)//2\n                    i3 = i1 + (j2 - j1)//2\n                    if 0 <= i3 < h and grille[i3][j3] == \"+\" and meme_diag1[(i1,j1)] == meme_diag1[(i3,j3)] and meme_diag2[(i2,j2)] == meme_diag2[(i3,j3)] :\n                        nb_triangles += 1\n                    i3 = i1 - (j2 - j1)//2\n                    if 0 <= i3 < h and grille[i3][j3] == \"+\" and meme_diag2[(i1,j1)] == meme_diag2[(i3,j3)] and meme_diag1[(i2,j2)] == meme_diag1[(i3,j3)] :\n                        nb_triangles += 1\n                j3 = j1\n                i3 = i1 + (j2 - j1)\n                if 0 <= i3 < h and grille[i3][j3] == \"+\" and meme_colonne[(i1,j1)] == meme_colonne[(i3,j3)] and meme_diag2[(i2,j2)] == meme_diag2[(i3,j3)] :\n                    nb_triangles += 1\n                i3 = i1 - (j2 - j1)\n                if 0 <= i3 < h and grille[i3][j3] == \"+\" and meme_colonne[(i1,j1)] == meme_colonne[(i3,j3)] and meme_diag1[(i2,j2)] == meme_diag1[(i3,j3)] :\n                    nb_triangles += 1\n                j3 = j2\n                i3 = i2 + (j2 - j1)\n                if 0 <= i3 < h and grille[i3][j3] == \"+\" and meme_colonne[(i2,j2)] == meme_colonne[(i3,j3)] and meme_diag1[(i1,j1)] == meme_diag1[(i3,j3)] :\n                    nb_triangles += 1\n                i3 = i2 - (j2 - j1)\n                if 0 <= i3 < h and grille[i3][j3] == \"+\" and meme_colonne[(i2,j2)] == meme_colonne[(i3,j3)] and meme_diag2[(i1,j1)] == meme_diag2[(i3,j3)] :\n                    nb_triangles += 1\n            elif (j1 == j2 and (i1 + i2)%2 == 0 and meme_colonne[(i1,j1)] == meme_colonne[(i2,j2)]) :\n                i3 = (i1 + i2)//2\n                j3 = j1 + (i2 - i1)//2\n                if 0 <= j3 < w and grille[i3][j3] == \"+\" and meme_diag1[(i1,j1)] == meme_diag1[(i3,j3)] and meme_diag2[(i2,j2)] == meme_diag2[(i3,j3)] :\n                    nb_triangles += 1\n                j3 = j1 - (i2 - i1)//2\n                if 0 <= j3 < w and grille[i3][j3] == \"+\" and meme_diag2[(i1,j1)] == meme_diag2[(i3,j3)] and meme_diag1[(i2,j2)] == meme_diag1[(i3,j3)] :\n                    nb_triangles += 1\n\nprint(nb_triangles)\nprint(nb_rectangles)\n\n",
