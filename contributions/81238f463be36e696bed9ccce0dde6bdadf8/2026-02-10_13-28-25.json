      "solution": "#include <iostream>\n#include <string>\n#include <vector>\n#include <algorithm>\n\nusing namespace std;\n\n/**\n * Auto-generated code below aims at helping you parse\n * the standard input according to the problem statement.\n **/\n\nint main()\n{\n    int quoolean;\n    cin >> quoolean; cin.ignore();\n\n    int ones = 0, zeros = 0;\n    for (int i = 0; i < 8; i++)\n    {\n        if (quoolean & 0x1 << i) ones++;\n        else zeros++;\n    }\n    string output = \"\";\n    if (ones == 8) output = \"true\";\n    else if (zeros == 8) output = \"false\";\n    else if (ones >= zeros) output = \"trueish\";\n    else output = \"falsish\";\n    cout << output << endl;\n}",
