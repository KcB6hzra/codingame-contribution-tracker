      "topics": [],
      "solution": "#include <iostream>\r\n#include <string>\r\n#include <vector>\r\n#include <algorithm>\r\n#include <sstream>\r\n#include <set>\r\n#include <map>\r\n#include <limits>\r\n#include <numeric>\r\n#include <regex>\r\n#include <cmath>\r\n#include <iomanip>\r\n#include <fstream>\r\n#include <chrono>\r\n#include <queue>\r\n\r\nusing namespace std;\r\n\r\n// returns the value of i that minimizes abs(index - i) while satisfying (available[i] > 0 && (lower ? i < index : i > index)), or -1 if there is no such i\r\nint findAvailable(const array<int, 10>& available, int index, bool lower)\r\n{\r\n    if(lower)\r\n    {\r\n        for(int i = index - 1; i >= 0; --i)\r\n            if(available[i] > 0)\r\n                return i;\r\n    }\r\n    else\r\n    {\r\n        for(int i = index + 1; i <= 9; ++i)\r\n            if(available[i] > 0)\r\n                return i;\r\n    }\r\n    return -1;\r\n}\r\n\r\n/* returns the closest integer to N using the available digits\r\n * the sum of values in \"available\" must be equal to N.size()\r\n * if \"lower\" is true, the function will return the greatest possible integer <= N\r\n * if \"lower\" is false, the function will return the lowest possible integer >= N\r\n * if there is no matching integer, the function will return an empty string */\r\nstring findClosest(const string& N, array<int, 10> available, bool lower)\r\n{\r\n    string res;\r\n    bool goBack = false;\r\n    for(char c : N)\r\n    {\r\n        int digit = c - '0';\r\n        if(available[digit] > 0)\r\n        {\r\n            res += c;\r\n            --available[digit];\r\n        }\r\n        else\r\n        {\r\n            int nextDigit = findAvailable(available, digit, lower);\r\n            if(nextDigit == -1)\r\n            {\r\n                goBack = true;\r\n            }\r\n            else\r\n            {\r\n                res += (char)('0' + nextDigit);\r\n                --available[nextDigit];\r\n            }\r\n\r\n            break;\r\n        }\r\n    }\r\n\r\n    if(res.size() == N.size()) // res == N\r\n        return res;\r\n\r\n    if(goBack)\r\n    {\r\n        int i;\r\n        for(i = (int)res.size() - 1; i >= 0; --i)\r\n        {\r\n            int digit = res[i] - '0';\r\n            ++available[digit];\r\n            int newDigit = findAvailable(available, digit, lower);\r\n            if(newDigit == -1)\r\n            {\r\n                res.pop_back();\r\n            }\r\n            else\r\n            {\r\n                res[i] = (char)('0' + newDigit);\r\n                --available[newDigit];\r\n                break;\r\n            }\r\n        }\r\n\r\n        if(i == -1) // no integer <= / >= can be made from the digits\r\n            return \"\";\r\n    }\r\n\r\n    if(lower)\r\n    {\r\n        for(int i = 9; i >= 0; --i)\r\n            res += string(available[i], (char)('0' + i));\r\n    }\r\n    else\r\n    {\r\n        for(int i = 0; i <= 9; ++i)\r\n            res += string(available[i], (char)('0' + i));\r\n    }\r\n    return res;\r\n}\r\n\r\nint main()\r\n{\r\n    ios::sync_with_stdio(false);\r\n    cin.tie(nullptr);\r\n    cout.tie(nullptr);\r\n\r\n    string N, M;\r\n    cin >> N >> M;\r\n\r\n    string answer;\r\n\r\n    if(M.size() < N.size())\r\n    {\r\n        /* If M doesn't have enough digits, the answer is their highest permutation.\r\n         * This works because N can never start with a zero, so the highest permutation of M will still be < N. */\r\n        sort(M.rbegin(), M.rend());\r\n        answer = move(M);\r\n    }\r\n    else\r\n    {\r\n        // count available digits in M\r\n        array<int, 10> available = {};\r\n        for(char c : M)\r\n            ++available[c - '0'];\r\n\r\n        // add leading zeros to N so that N.size() == M.size()\r\n        if(N.size() < M.size())\r\n            N = string(M.size() - N.size(), '0') + N;\r\n\r\n        // compute highest permutation <= N, and lowest permutation >= N\r\n        string lower = findClosest(N, available, true), upper = findClosest(N, available, false);\r\n        if(lower.empty())\r\n        {\r\n            answer = move(upper);\r\n        }\r\n        else if(upper.empty())\r\n        {\r\n            answer = move(lower);\r\n        }\r\n        else\r\n        {\r\n            // find out whether lower or upper is closest to N\r\n\r\n            bool bestFound = false, lowerIsBest = false;\r\n            int diff = 0; // how much closer upper is to N than lower\r\n            for(size_t i = 0; i < N.size(); ++i)\r\n            {\r\n                diff = (diff * 10) + 2 * (N[i] - '0') - (lower[i] - '0') - (upper[i] - '0');\r\n                // stop computation as soon as possible, to avoid diff overflowing\r\n                if(diff >= 2)\r\n                {\r\n                    /*  Upper is definitely better than lower.\r\n                     * Example: if we have as prefixes: N = \"5\", lower = \"2\" and upper = \"6\", then diff = 2\r\n                     * Even if later on we get N = \"500000...\", lower = \"2999999...\", upper = \"6999999\",\r\n                     * N - lower can never get as small as upper - N */\r\n                    bestFound = true;\r\n                    lowerIsBest = false;\r\n                    break;\r\n                }\r\n                else if(diff <= -2)\r\n                {\r\n                    /*  Lower is definitely better than upper.\r\n                     * Example: if we have as prefixes: N = \"5\", lower = \"4\" and upper = \"8\", then diff = -2\r\n                     * Even if later on we get N = \"5999999...\", lower = \"4000000...\", upper = \"8000000\",\r\n                     * upper - N can never get as small as N - lower */\r\n                    bestFound = true;\r\n                    lowerIsBest = true;\r\n                    break;\r\n                }\r\n            }\r\n            if(!bestFound)\r\n                lowerIsBest = diff <= 0;\r\n\r\n            answer = move(lowerIsBest ? lower : upper);\r\n        }\r\n    }\r\n\r\n    // remove leading zeros\r\n    auto start = answer.find_first_not_of('0');\r\n    if(start != 0)\r\n        answer = answer.substr(start);\r\n\r\n    cout << answer << endl;\r\n\r\n    return 0;\r\n}",
