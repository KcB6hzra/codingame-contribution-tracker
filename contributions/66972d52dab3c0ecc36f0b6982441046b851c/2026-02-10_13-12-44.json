      "solution": "using System; \nusing System.Linq; \nusing System.IO; \nusing System.Text; \nusing System.Collections; \nusing System.Collections.Generic; \nusing System.Text.RegularExpressions;\n\npublic class Program\n{\n\tpublic static List<Purse> PURSES = new List<Purse>(); \n\tpublic static void Main()\n\t{\n        int howMany = int.Parse(Console.ReadLine());\n        for (int whichPurse = 0 ; whichPurse < howMany ; whichPurse++)\n        {\n            string line = Console.ReadLine();\n            string line2 = line.Replace(\", \",\",\");\n            var tempRay = line2.Split();\n            string col = tempRay[1];\n            string coins = tempRay[4];\n            Purse PURSE = new Purse(col, coins);\n            PURSES.Add(PURSE);\n        }\n        Purse BIGGESTPURSE = Purse.FindBiggestPurse(PURSES);\n\n        Console.WriteLine(BIGGESTPURSE.PurseColor);\n\t}\n\t\n\tpublic class Purse\n\t{\n\t\tpublic string PurseColor;\n\t\tpublic List<int> CoinsList = new List<int>();\n\t\tpublic int TotalValue;\n\t\t\n\n\t\tpublic Purse(string col, string strCoins)\n\t\t{\n\t\t\tPurseColor = col;\n\t\t\tPutCoinsIntoCoinsList(strCoins);\n\t\t\tCalcTotalValue();\n\t\t}\n        \n\t\t\n\t\tpublic void PutCoinsIntoCoinsList(string c)\n\t\t{\n\t\t\tvar strRay = c.Split(',');\n\t\t\tforeach (var item in strRay)\n\t\t\t{\n\t\t\t\tint i = int.Parse(item);\n\t\t\t\tCoinsList.Add(i);\n\t\t\t}\n\t\t}\n\n\t\tpublic void CalcTotalValue()\n\t\t{\n\t\t\tint tot = 0 ;\n\t\t\tforeach (var aCoin in CoinsList)\n\t\t\t{\n\t\t\t\ttot += aCoin;\n\t\t\t}\n\t\t\tTotalValue = tot;\t\n\t\t}\n\t\t\n\t\tpublic static Purse FindBiggestPurse(List<Purse> purseList)\n\t\t{\n\t\t\tPurse biggestPurse = purseList[0];\n\t\t\tforeach (Purse p in purseList)\n\t\t\t{\n\t\t\t\tif (p.TotalValue > biggestPurse.TotalValue)\n\t\t\t\t{\n\t\t\t\t\tbiggestPurse = p;\n\t\t\t\t}\n\t\t\t}\n\t\t\treturn biggestPurse;\n\t\t}\n\t}\n}",
