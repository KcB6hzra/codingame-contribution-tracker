      "solution": "a = input()\nb = input()\n\nd = dict()\n\nsep = \",\"\n\nmlip = 0\nmlfp = 0\n\nfor n in (a, b):\n    d[n] = {\"ip\":\"0\", \"fp\":\"0\"}\n    \n    i = n.find(sep)\n    if i == -1:\n        d[n][\"ip\"] = n\n    elif i == 0:\n        d[n][\"fp\"] = n[i+1:]\n    else:\n        d[n][\"ip\"] = n[:i]\n        d[n][\"fp\"] = n[i+1:]\n        \n    mlip = max(mlip, len(d[n][\"ip\"]))\n    mlfp = max(mlfp, len(d[n][\"fp\"]))\n\nfor n in (a, b):\n    out = str()\n    out += \" \" * (mlip - len(d[n][\"ip\"]))\n    out += d[n][\"ip\"]\n    \n    out += sep\n    out += d[n][\"fp\"]\n    out += \"0\" * (mlfp - len(d[n][\"fp\"]))\n        \n    print(out)\n    ",
