      "solution": "import java.util.*;\r\nimport java.io.*;\r\nimport java.math.*;\r\n\r\nclass Solution {\r\n\r\n    public static void main(String args[]) {\r\n        Scanner in = new Scanner(System.in);\r\n        int S = in.nextInt();\r\n        int P = in.nextInt();\r\n        int A = (S-(int)Math.sqrt(S*S-4*P))/2;\r\n        int B = S-A;\r\n        System.out.println(A+\" \"+B);\r\n    }\r\n}",
