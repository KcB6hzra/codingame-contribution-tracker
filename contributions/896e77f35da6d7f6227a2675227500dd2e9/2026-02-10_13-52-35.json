      "solution": "import java.util.*;\r\nimport java.math.*;\r\nimport java.io.*;\r\nimport java.awt.*;\r\nclass Solution {\r\n    public static void main(String args[]) {\r\n        Scanner sc = new Scanner(System.in);\r\n        int N=sc.nextInt();\r\n        sc.next();\r\n        int M=sc.nextInt();\r\n        if (M==0) {System.out.println(\"INVALID\"); return;}\r\n        int HCF=getHCF(N,M);\r\n        System.out.println((N/HCF)+\" / \"+(M/HCF));\r\n    }\r\n    static int getHCF(int N,int M) {\r\n        try {return getHCF(M%N,N);}\r\n        catch (ArithmeticException e) {return M;}\r\n    }\r\n}",
