      "solution": "using System; using System.Linq; using System.IO; using System.Text; using System.Collections; using System.Collections.Generic;\n\npublic class Program\n{\n\tconst string flipPattern2 = \"(){}[]<>\";    // bookFlip\n\tconst string flipPattern3 = \"^vAVwmWMun\";  // downFlip\n\tconst string flipPattern4 = \"/\\\\\";         // sameDiag\n\tpublic static char[,] FINALRAY;\n\t\n\tpublic static void Main()\n\t{\t\n        int N = Int16.Parse(Console.ReadLine());\n\t\tList<String> aQtr = new List<String>();\n\t\tfor (int i = 0 ; i < N ; i++)\n\t\t{\n\t\t\taQtr.Add(Console.ReadLine());\n\t\t}\n\t\t\n\t\tSquareModel aTile = new SquareModel(aQtr);\n\t\tTileFloor tf = new TileFloor(aTile);\n\t\ttf.PrintTileFloor(); //aTile.PrintQtrSquare();\n\t}\n\t\n\tpublic class SquareModel\n\t{\n\t\tpublic List<string> Lines;\n        public int QtrSize;\n\t\tpublic int FullSize;\n\t\tpublic char[,] TopLeftQtr;\n\t\tpublic char[,] FullSquare;\n\n\t\t\n\t\tpublic SquareModel(List<string> lines)\n\t\t{\n\t\t\tLines = lines;\n\t\t\tQtrSize = lines[0].Length;\n\t\t\tFullSize = 2 * QtrSize - 1;\n\t\t\t\n\t\t\tTopLeftQtr = new char[QtrSize, QtrSize];\t\n\t\t\tFullSquare = new char[FullSize , FullSize];\n\t\t\t\n\t\t\tPopulateQtrSquare();\n\t\t\tInitializeFullSquare();\n\t\t\tPopulateFullSquare();\n\t\t}\n\t\t\n\t\tpublic void PopulateQtrSquare()\n\t\t{\n\t\t\tfor (int lin = 0 ; lin < QtrSize ; lin++)\n\t\t\t{\n\t\t\t\tfor (int pos = 0 ; pos < QtrSize ; pos++)\n\t\t\t\t{\n\t\t\t\t\tTopLeftQtr[lin,pos] = Lines[lin][pos];\n\t\t\t\t}\n\t\t\t}\n\t\t}\n\t\t\n\t\n\t\tpublic void InitializeFullSquare()\n\t\t{\n\t\t\tfor (int lin = 0 ; lin < FullSize; lin++)\n\t\t\t{\n\t\t\t\tfor (int pos = 0 ; pos < FullSize ; pos++) \n\t\t\t\t{\n\t\t\t\t\tFullSquare[lin, pos] =  ' ';\n\t\t\t\t}\n\t\t\t}\n\t\t}\n\n\t\t\n\t\tpublic void PopulateSingleQtr(int whichQtr)\n\t\t{\n\t\t\tint biFull = FullSize - 1;                 //biggest Index Of Full\n\t\t\tfor (int lin = 0 ; lin < QtrSize ; lin++)\n\t\t\t{\n\t\t\t\tfor (int pos = 0 ; pos < QtrSize ; pos++)\n\t\t\t\t{\n\t\t\t\t\tchar ch1 = TopLeftQtr[lin,pos];\n\t\t\t\t\tbool pattern2 = flipPattern2.Contains(ch1);  // bookFlip (sideways)\n\t\t\t\t\tbool pattern3 = flipPattern3.Contains(ch1);  // downFlip\n\t\t\t\t\tbool pattern4 = flipPattern4.Contains(ch1);\t // sameDiag\n\n\t\t\t\t\tchar ch2 = FindMirror(ch1, flipPattern2 + flipPattern3 + flipPattern4);\n\t\t\t\t\t\n\t\t\t\t\tif (whichQtr == 1)\n\t\t\t\t\t{\n\t\t\t\t\t\tFullSquare[lin, pos] = ch1;\n\t\t\t\t\t}\n\t\t\t\t\t\n\t\t\t\t\tif (whichQtr == 2) // bookFlip, sameDiag\n\t\t\t\t\t{\n\t\t\t\t\t\tFullSquare[lin, biFull - pos] = ch1;\n\t\t\t\t\t\tif (pattern2 || pattern4) FullSquare[lin, biFull - pos] = ch2;\t\n\t\t\t\t\t}\n\t\t\t\t\t\n\t\t\t\t\tif (whichQtr == 3) // downFlip, sameDiag\n\t\t\t\t\t{\n\t\t\t\t\t\tFullSquare[biFull - lin,  pos]  = ch1;\n\t\t\t\t\t\tif (pattern3 || pattern4) FullSquare[biFull - lin, pos] = ch2;\n\t\t\t\t\t}\n\t\t\t\t\t\n\t\t\t\t\tif (whichQtr == 4) //bookFlip, downFlip\n\t\t\t\t\t{\n\t\t\t\t\t\tFullSquare[biFull-lin, biFull-pos] = ch1;\n\t\t\t\t\t\tif (pattern2 || pattern3) FullSquare[biFull - lin, biFull-pos] = ch2;\n\t\t\t\t\t}\n\n\t\t\t\t\t/*\n\t\t\t\t\tassumes quardrants are like\n\t\t\t\t\t12\n\t\t\t\t\t34\n\t\t\t\t\t*/\n\t\t\t\t}\n\t\t\t}\n\t\t}\n\t\t\n\t\tpublic void PopulateFullSquare()             \n\t\t{\n\t\t\tPopulateSingleQtr(1); PopulateSingleQtr(2); PopulateSingleQtr(3); PopulateSingleQtr(4);\n\t\t}\n\t\n\t\tpublic void PrintFullSquare()\n\t\t{\n\t\t\tfor (int lin = 0 ; lin < FullSize; lin++)\n\t\t\t{\n\t\t\t\tfor (int pos = 0 ; pos < FullSize ; pos++)\n\t\t\t\t{\n\t\t\t\t\tchar ch = FullSquare[lin,pos];\n\t\t\t\t\tConsole.Write(ch + \"\");\n\t\t\t\t}\n\t\t\t\tConsole.WriteLine();\n\t\t\t}\n\t\t}\n\n\t\tpublic char FindMirror(char ch1, string strOptions)\n\t\t{\n\t\t\tchar[] OptionsRay = strOptions.ToCharArray();\t\n\t\t\tchar ch2 = ch1;\n\t\t\t\n\t\t\tint index = Array.IndexOf(OptionsRay, ch1);\n\t\t\tif (index != -1)\n\t\t\t{\n\t\t\t\tif (index % 2 == 0) ch2 = OptionsRay[index+1];\n\t\t\t\tif (index % 2 == 1) ch2 = OptionsRay[index-1];\n\t\t\t}\n\t\t\treturn ch2;\n\t\t}\n\t}\n\t\n\tpublic class TileFloor\n\t{\n\t\tpublic SquareModel Tile;\n\t\tpublic int TileSiz;\n\t\tpublic int TileSizPLUS;\n\t\tpublic int FloorSiz;\n\t\t\t\t\t\n\t\tpublic TileFloor(SquareModel aTile)\n\t\t{\n\t\t\tTile = aTile;\n\t\t\tTileSiz = Tile.FullSize;\n\t\t\tTileSizPLUS = TileSiz + 1; //allows for a left and top border\n\t\t\tFloorSiz = 2 * TileSizPLUS + 1;\n\t\t\tFINALRAY = new char[FloorSiz,FloorSiz];\n\t\t\tInitializeTileFloor();\n\t\t\t\n\t\t\tfor (int lin = 0 ; lin < FloorSiz; lin++)\n\t\t\t{\n\t\t\t\tfor (int pos = 0 ; pos < FloorSiz ; pos++)\n\t\t\t\t{\n\t\t\t\t\tint remLin = lin % TileSizPLUS;\n\t\t\t\t\tint remPos = pos % TileSizPLUS;\n \n\t\t\t\t\tif (remLin == 0) FINALRAY[lin,pos] = '-';\n\t\t\t\t\tif (remPos == 0) FINALRAY[lin,pos] = '|';\n\t\t\t\t\tif (remLin + remPos == 0) FINALRAY[lin,pos] = '+';\n\t\t\t\t}\n\t\t\t}\n\t\t\tPopulateFloor(0,0); PopulateFloor(0,1); PopulateFloor(1,0); PopulateFloor(1,1);\n\t\t}\n\t\t\n\t\tpublic void InitializeTileFloor()\n\t\t{\n\t\t\tfor (int lin = 0 ; lin < FloorSiz; lin++)\n\t\t\t{\n\t\t\t\tfor (int pos = 0 ; pos < FloorSiz ; pos++)\n\t\t\t\t{\n\t\t\t\t\tFINALRAY[lin, pos] =  ' ';\n\t\t\t\t}\n\t\t\t}\n\t\t}\n\t\t\n\t\tpublic void PrintTileFloor()\n\t\t{\n\t\t\tfor (int lin = 0 ; lin < FloorSiz ; lin++)\n\t\t\t{\n\t\t\t\tfor (int pos = 0 ; pos < FloorSiz  ; pos++)\n\t\t\t\t{\n\t\t\t\t\tchar ch = FINALRAY[lin,pos];\n\t\t\t\t\tConsole.Write(ch);\n\t\t\t\t}\n\t\t\t\tConsole.WriteLine();\n\t\t\t}\n\t\t}\n\t\tpublic void PopulateFloor(int offsetLin, int offsetPos)\n\t\t{\n\t\t\tfor (int lin = 0 ; lin < TileSiz; lin++)\n\t\t\t{\n\t\t\t\tfor (int pos = 0 ; pos < TileSiz ; pos++)\n\t\t\t\t{\n\t\t\t\t\tint LIN = lin + 1 + (offsetLin * TileSizPLUS);\n\t\t\t\t\tint POS = pos + 1 + (offsetPos * TileSizPLUS);\n\t\t\t\t\tFINALRAY[LIN, POS] = Tile.FullSquare[lin, pos]; \n\t\t\t\t}\n\t\t\t}\n\t\t}\t\n\t}\n}\n",
      "statement": "You are a manufacturer and installer of Floor-Tiles.\n\nThe beauty of Tiles comes from their symmetry, in that the quadrants of a Tile are mirror/reflections of each other.\n\nSo, when you get an order for a Tile, only the pattern of the top left quadrant (plus a little more, so you can see the center characters) are the input.  Graphically explained here:\n`☑☑☑☐☐\n☑☑☑☐☐\n☑☑☑☐☐\n☐☐☐☐☐\n☐☐☐☐☐`From that limited input, your task is to:\nCreate a full Tile\nAnd display a 2x2 floor (i.e., a floor consisting of 4 of those Tiles)\n\n<<DETAILS:>>\n➤ Each Tile is a square; the input therefore is also a square\n➤ The input square shows the <<top left>> character of the full Tile, all the way to the <<middle>> character of the full Tile\n➤ The input has a side-length of [[N]]; therefore the full Tile has a side-length of 2[[N]]-1\n➤ The full floor needs a grout border, and grout separating the individual Tiles.  Grout is represented by {{-}} on top/bottom, {{|}} on left/right, and {{+}} on corners\n\n<<Flip/Mirror patterns:>>\nGenerally, a character appearing in the first quadrant just replicates in the other quadrants.\n\nHowever, for these special characters, you will need to sometimes print the opposite-mate of it:\n• {{() {} [] <>}}  If any of these are on the left side, then show its opposite-mate on the right side \n• {{^v AV wm WM un}}  If any of these are on the top half, then show its opposite-mate on the bottom half\n• {{/\\}}  If either of these is on the top left quadrant, then show its opposite-mate on the two adjacent quadrants\n\n(The opposite-mate is the one beside it in the character-pair as shown)",
