      "solution": "#include <iostream>\n#include <vector>\n#include <queue>\nusing namespace std;\n#define inRange(x, y, r, c) (x >= 0 && x < r && y >= 0 && y < c)\n\nvector< vector<int> > getMaze(int n){\n\tvector< vector<int> > v;\n\tint r = 2 * n - 1, c = r;\n\tqueue<int> q;\n\tint a[] = {-1, -1, -1, 0, 0, 1, 1, 1}, b[] = {-1, 0, 1, -1, 1, -1, 0, 1};\n\tv.resize(r);\n\tfor(int i=0;i<r;i++){\n\t\tv[i].resize(c);\n\t\tfill(v[i].begin(), v[i].end(), -1);\n\t}\n\tv[r/2][c/2] = 1;\n\tq.push(r/2);\n\tq.push(c/2);\n\twhile(!q.empty()){\n\t\tint x = q.front();\n\t\tq.pop();\n\t\tint y = q.front();\n\t\tq.pop();\n\t\tfor(int i=0;i<8;i++){\n\t\t\tint tx = x + a[i];\n\t\t\tint ty = y + b[i];\n\t\t\tif(inRange(tx, ty, r, c) && v[tx][ty] == -1){\n\t\t\t\tv[tx][ty] = v[x][y] + 1;\n\t\t\t\tq.push(tx);\n\t\t\t\tq.push(ty);\n\t\t\t}\n\t\t}\n\t}\n\treturn v;\n}\n\nvoid display(vector< vector<int> > v){\n\tfor(int i=0;i<v.size();i++){\n\t\tfor(int j=0;j<v[i].size();j++)\n\t\t\tcout << v[i][j];\n\t\tcout << endl;\n\t}\n}\n\nint main() {\n\tint n;\n\tcin >> n;\n\tdisplay(getMaze(n));\n\treturn 0;\n}",
