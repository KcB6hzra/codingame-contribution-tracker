      "solution": "<?php\nerror_reporting(0);\nfscanf(STDIN, \"%d\", $n);\n\nfunction pfactor($n){\n\t$d=2;\n\t$factors = array();\n\t$dmax = floor(sqrt($n));\n\t$sieve = array();\n\t$sieve = array_fill(1, $dmax,1);\n\tdo{\n\t\t$r = false;\n\t\twhile ($n%$d==0){\n\t\t\t$factors[$d]++;\n\t\t\t$n/=$d;\n\t\t\t$r = true;\n\t\t}\n\t\tif ($r){\n\t\t\t$dmax = floor(sqrt($n));\n\t\t}\n\t\tif ($n>1){\n\t\t\tfor ($i=$d;$i<=$dmax;$i+=$d){\n\t\t\t\t$sieve[$i]=0;\n\t\t\t}\n\t\t\tdo{\n\t\t\t\t$d++;\n\t\t\t}while ($sieve[$d]!=1 && $d<$dmax);\n\t\t\tif ($d>$dmax){\n\t\t\t\t$factors[$n]++;\n\t\t\t}\n\t\t}\n\t}while($n>1 && $d<=$dmax);\n\treturn $factors;\n}\n\n\n$factors = [];\nforeach (pfactor($n) as $b=>$e)\n    for($i = 0; $i < $e; $i ++)\n        $factors[] = $b;\n        \n$h = [];\nfunction buildh($f,$a=1,$b=1) {\n\tglobal $h;\n\tif(count($f) == 0) {\n\t\tif($a > 1 && $b > 1 && $a%2 == $b%2) {\n\t\t\t$h[$a] = $b;\n\t\t\t$h[$b] = $a;\n\t\t}\n\t\treturn;\n\t}\n\t$t = array_pop($f);\n\tbuildh($f,$a*$t,$b);\n\tbuildh($f,$a,$b*$t);\n}\n\nbuildh($factors);\nkrsort($h);\n//var_dump($h);\n\n\n$non = 0;\n$ans = \"\";\nforeach($h as $k => $v) {\n\tif($k <= $v) {\n\t    $d = $k%2 == 1 ? ($k-1)/2 : $k/2;\n\t    $l = $k%2 == 1 ? $v-$d*2 : $v-$d*2+1;\n\t    $r = $k%2 == 1 ? $v+$d*2 : $v+$d*2-1;\n\t\t$ans = \"$l $r\";\n\t\tbreak;\n\t} else {\n\t\t$non ++;\n\t}\n}\n\necho count($h)-$non,\"\\n\";\necho $ans;",
