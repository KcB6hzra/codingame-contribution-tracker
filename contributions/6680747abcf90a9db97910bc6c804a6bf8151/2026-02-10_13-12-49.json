      "solution": "use std::{io::{self, BufRead, BufReader}, collections::HashMap};\n\nfn simple(n: i32, cache: &mut HashMap<i32, i32>) -> i32 {\n    if n == 0 {\n        return 1;\n    }\n\n    if let Some(v) = cache.get(&n) {\n        return *v;\n    }\n\n    let result;\n    if n >= 2 {\n        result = simple(n - 1, cache) + simple(n - 2, cache);\n    } else {\n        result = simple(n - 1, cache);\n    }\n\n    cache.insert(n, result);\n\n    result\n}\n\nfn main() {\n    let mut buf = BufReader::new(io::stdin()).lines();\n    let mut read_line = || buf.next().unwrap().unwrap().trim().to_string();\n\n    let n: i32 = read_line().parse().unwrap();\n\n    let mut cache: HashMap<i32, i32> = HashMap::new();\n    let result = simple(n, &mut cache);\n\n    println!(\"{}\", result);\n}",
