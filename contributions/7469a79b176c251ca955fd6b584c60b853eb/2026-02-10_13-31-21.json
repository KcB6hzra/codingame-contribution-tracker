      "solution": "using System;\nusing System.IO;\nusing System.Text;\nusing System.Collections;\nusing System.Collections.Generic;\nusing System.Linq;\nusing System.Text.RegularExpressions;\n\nclass Solution\n{\n    static void Main(string[] args)\n    {\n        var game = new Game();\n        // For Debugging\n        //Console.Error.WriteLine(game.DebugString());    \n        Console.WriteLine(game.FindWally());\n    }\n}\npublic class Game\n{\n    public string Name { get; set; }\n    public int WallyW { get; set; }\n    public int WallyH { get; set; }\n    public string Wally { get; set; }\n\n    public int MapW { get; set; }\n    public int MapH { get; set; }\n    public string Map { get; set; }\n    \n\n    public Game(){\n        string[] inputs = Console.ReadLine().Split(' ');\n        WallyW = int.Parse(inputs[0]);\n        WallyH = int.Parse(inputs[1]);\n        Wally = \"\";\n        var wallyRows = new List<string>();\n        for (int i = 0; i < WallyH; i++)\n        {\n            wallyRows.Add(Console.ReadLine());\n        }\n        Wally = string.Join(\"\\n\",wallyRows);\n\n        inputs = Console.ReadLine().Split(' ');\n        MapW = int.Parse(inputs[0]);\n        MapH = int.Parse(inputs[1]);\n        var mapRows = new List<string>();\n\n        for (int i = 0; i < MapH; i++)\n        {\n            mapRows.Add(Console.ReadLine());\n        }\n        Map = string.Join(\"\\n\",mapRows);            \n    }\n\n    public string DebugString()\n    {\n        return $\"{WallyW} {WallyH}\\n\" \n            + Wally +\"\\n\" \n            + $\"{MapW} {MapH}\\n\"\n            + Map \n            ;\n    }\n\n    public string FindWally()\n    {\n\n        var mapRealW = MapW + 1; // row + \\n\n\n        var rowSpacing = mapRealW - WallyW; // Distance between 2 wally rows in picture\n\n        // Create Regex pattern\n        var pattern =\n            string.Join(\n                new StringBuilder(8* rowSpacing).Insert(0,@\"(?:.|\\n)\", rowSpacing ).ToString(), \n                Wally.Split('\\n') // lines Split\n                .Select(\n                    row => string.Join(\n                        \".\",   // JOIN with dot for matching anything\n                        row.Split(' ') // Split on space for not escape it\n                            .Select(c=>Regex.Escape(c)) // escape all others chars \n                    )\n                )\n            );\n        var match = new Regex(pattern).Match(Map);\n        if (match.Success)\n        {\n            return match.Index % mapRealW + \" \" + (match.Index / mapRealW);\n        }\n        else\n        {\n            return \"NOT HERE\";\n        }\n    }\n}\n",
      "statement": "You will get a representation of Wally and a Picture.\n\nYou must find Wally's position inside the picture. The top left corner of the picture is [[(x, y)]]={{(0,0)}}.\n\n<<If Wally's representation contains spaces, the picture may contain others characters at these positions.>>\n\n<<All non-space characters of Wally will be visible in the picture.>>\n\n________________________________________\n* ASCII ART representations of wally are from :\n - https://ascii.co.uk/art/men\n - https://asciiart.website/index.php?art=comics/dilbert",
