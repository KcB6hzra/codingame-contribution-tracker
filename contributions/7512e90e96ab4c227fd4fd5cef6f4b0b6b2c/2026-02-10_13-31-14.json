      "solution": "#include <vector>\n#include <iostream>\nusing namespace std;\n\nint main() {\n\tvector<vector<int> > A(5, vector<int>(5));\n\tint blankr = -1, blankc = -1;\n\tfor(int i = 0; i < 5; ++i) {\n\t\tfor(int j = 0; j < 5; ++j) {\n\t\t\tcin >> A[i][j];\n\t\t\tif(A[i][j] == -1) {\n\t\t\t\tblankr = i;\n\t\t\t\tblankc = j;\n\t\t\t}\n\t\t}\n\t}\n\tint targetr = (blankr != 0 ? 0 : 1);\n\tlong long targetsum = 0, blankrowsum = 0;\n\tfor(int i = 0; i < 5; ++i) {\n\t\ttargetsum += A[targetr][i];\n\t\tif(i != blankc) {\n\t\t\tblankrowsum += A[blankr][i];\n\t\t}\n\t}\n\tcout << targetsum - blankrowsum << endl;\n\treturn 0;\n}",
