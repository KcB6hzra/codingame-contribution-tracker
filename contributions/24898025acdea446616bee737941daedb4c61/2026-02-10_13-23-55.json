      "solution": "import java.util.*;\nimport java.io.*;\nimport java.math.*;\n\nclass Solution {\n\n    public static void main(String args[]) {\n        Scanner in = new Scanner(System.in);\n        String s = in.nextLine();\n\n        String arr[] = s.split(\" \");\n        ArrayList<String> listWithAnswers = new ArrayList<String>();\n\n        int occurrence = 0;\n        int check1;\n        int result = 0;\n\n        for (int i = 0; i < arr.length; i++) {\n\n            for (int j = 0; j < arr.length; j++) {\n                if (arr[i].equals(arr[j])) {\n                    occurrence++;\n                }\n            }\n            if ((occurrence % 2) != 0) {\n                listWithAnswers.add(arr[i]);\n            }\n            occurrence = 0;\n        }\n\n        result = Integer.parseInt(listWithAnswers.get(0));\n        for (int i = 0; i < listWithAnswers.size(); i++) {\n            check1 = Integer.parseInt(listWithAnswers.get(i));\n            result = Math.min(check1, result);\n        }\n\n        System.out.println(result);\n    }\n}",
