      "solution": "<?php\n\nfscanf(STDIN, \"%s\", $F);\nfscanf(STDIN, \"%d %d\", $xmin,$xmax);\n\n$s=explode(\"=\",$F);\n$nomfonction=$s[0];\n\n$pos1=strpos($s[1],\"x²\");\nif($pos1!==false)\n{\n    $a=substr($s[1],0,$pos1);\n    if($a==\"\")\n        $a=1;\n    if($a==\"-\")\n        $a=-1;\n}\nelse\n{\n    $a=0;\n    $pos1=-3;\n}\n \n$pos2=strpos($s[1],\"x\",$pos1+3);\nif($pos2!==false)\n{\n    $b=substr($s[1],$pos1+3,$pos2-$pos1-3);\n    if($b==\"+\")\n        $b=1;\n    if($b==\"-\")\n        $b=-1;\n}\nelse\n{\n    $b=0;\n    $pos2=$pos1+2;\n}\n\n$c=substr($s[1],$pos2+1);\nif($c==\"\")\n    $c=0;\n\nfor($x=$xmin;$x<=$xmax;$x++)\n{\n    echo(str_replace(\"x\",$x,$nomfonction).\"=\".($a*$x*$x+$b*$x+$c).\"\\n\");\n}\n?>",
