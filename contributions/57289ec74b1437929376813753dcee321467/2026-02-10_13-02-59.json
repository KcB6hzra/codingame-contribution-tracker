      "solution": "cat > ./A << EOF\n#include<iostream>\n#include<gmpxx.h>\nusing namespace std;\nvoid binary_split(mpz_class &P,mpz_class &Q,mpz_class &T,int a,int b){\n mpz_class P2,Q2,T2;\n if(b==a+1){\n  P=(6*a-5);P*=(2*a-1);P*=(6*a-1);\n  Q=10939058860032000;\n  Q=Q*a*a*a;\n  if(!a){P=1;Q=1;}\n  T=P*(13591409+545140134L*a)*(1-2*(a&1));\n  return;}\n int m=(a+b)/2;\n binary_split(P,Q,T,a,m);\n binary_split(P2,Q2,T2,m,b);\n T=P*T2+T*Q2;\n P*=P2;\n Q*=Q2;}\nint main(){\n int INDEX,n;\n cin >> INDEX >> n; \n mpz_class P,Q,T,zz=100;\n binary_split(P,Q,T,0,70518);\n mpz_pow_ui(zz.get_mpz_t(),zz.get_mpz_t(),1000050);\n Q=(Q*sqrt(zz*10005)*426880)/T;\n cout<<Q.get_str().substr(INDEX,n)<<endl;}\nEOF\ng++ -o p -x c++ A -lgmp -lgmpxx\n./p",
