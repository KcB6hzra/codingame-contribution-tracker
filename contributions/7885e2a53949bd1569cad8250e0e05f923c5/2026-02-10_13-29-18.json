      "solution": "using System;\nusing System.Linq;\n\nclass Solution\n{\n    static void Main()\n    {\n        Console.ReadLine();\n        Int32[] numbers = Console.ReadLine().Split().Select(Int32.Parse).ToArray();\n        Console.ReadLine();\n        Int32[] indices = Console.ReadLine().Split().Select(Int32.Parse).ToArray();\n        \n        Int32 total = 0;\n\n        foreach (Int32 index in indices)\n        {\n            total += numbers[index];\n        }\n\n        Console.WriteLine(total);\n    }\n}",
