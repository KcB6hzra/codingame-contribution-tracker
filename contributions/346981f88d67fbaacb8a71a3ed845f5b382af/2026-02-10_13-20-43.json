      "solution": "#include <iostream>\n#include <string>\n#include <vector>\n#include <algorithm>\n\nusing namespace std;\n\nint main()\n{\n    int n;\n    cin >> n; cin.ignore();\n\n    vector<string> a;\n    int b = -1;\n    for (int i = 0; i < n; i++) {\n        long long num;\n        cin >> num; cin.ignore();\n        string s = to_string(num);\n        a.push_back(s);\n        if ((int) s.length() > b) {\n            b = s.length();\n        }\n    }\n\n    for (string i : a) {\n        if (i.length() < b) {\n            for (int j = 0; j < b - i.length(); j++) {\n                cout << \" \";\n            }\n        }\n        cout << i << endl;\n    }\n}",
