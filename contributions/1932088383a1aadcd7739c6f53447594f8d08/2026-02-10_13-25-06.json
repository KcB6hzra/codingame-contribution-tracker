      "solution": "using System;\nusing System.Linq;\nusing System.IO;\nusing System.Text;\nusing System.Collections;\nusing System.Collections.Generic;\n\nclass Solution\n{\n    static void Main(string[] args)\n    {\n        string plus = \"+\";\n        string pipe = \"|\";\n        var input = Console.ReadLine();\n        int sCount =  input.Count(f => f == 's'); //surrounding sea\n        int wCount =  input.Count(f => f == 'w'); //width of raft\n        int hCount =  input.Count(f => f == 'h'); //height of raft\n        int totalWidth = sCount*2 + wCount;\n        string sideWig = new String('~',sCount);\n        string topBotWig = new String('~',totalWidth);\n        \n        string temp1 = \"\";\n        string temp2 = \"\";\n        if (wCount > 2 )\n        {\n             temp1 = new String('-', wCount-2);\n             temp2 = new String(' ', wCount-2);\n        }\n        string topBotOfRaft = plus + temp1 + plus;\n        string midOfRaft = pipe + temp2 + pipe;\n        \n        for (int i = 0 ; i < sCount ; i++){\n            Console.WriteLine(topBotWig);\n        }\n        Console.WriteLine(sideWig + topBotOfRaft + sideWig);\n\n////////////////////////////////////////////////////////////////////\n        int internalH = hCount - 2;\n        for (int i = 0 ; i < internalH ; i++){\n            Console.WriteLine(sideWig + midOfRaft + sideWig);\n        }\n////////////////////////////////////////////////////////////////////\n\n        Console.WriteLine(sideWig + topBotOfRaft + sideWig);\n        for (int i = 0 ; i < sCount  ; i++){\n            Console.WriteLine(topBotWig);\n        }\n    }\n}",
