      "solution": "import java.util.*;\nimport java.io.*;\nimport java.math.*;\n\nclass Solution {\n\n    public static void main(String args[]) {\n        Scanner in = new Scanner(System.in);\n        int number = in.nextInt();\n        int a = number;\n        boolean invert = false;\n        for(int i = 0; i < number; i++){\n            if(invert) {\n                for (int j = a; j > 0; j--) {\n                    System.out.print(j);\n                }\n            } else{\n                for (int j = 1; j < a+1; j++) {\n                    System.out.print(j);\n                }\n            }\n            invert = !invert;\n            a--;\n            System.out.println(\"\");\n        }\n    }\n}",
