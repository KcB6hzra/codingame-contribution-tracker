      "solution": "using System;\nusing System.Linq;\nusing System.IO;\nusing System.Text;\nusing System.Collections;\nusing System.Collections.Generic;\n\nclass Solution\n{\n    static void Main(string[] args)\n    {\n        string D = Console.ReadLine();\n\n        string d = \"\";\n        \n        var decs = D.Split(' ');\n        \n        foreach (var de in decs)\n        {\n            if (de.Length > 0)\n            {\n                string num = de.Substring(0,de.Length-1);\n                string c = de.Substring(de.Length-1,1);\n                //Console.WriteLine(de + \" -> \" + num + \" -> \" + c);\n                int n = int.Parse(num);\n                for (int i=0;i<n;i++)\n                    d += c;\n            }\n        }\n        Console.WriteLine(d);\n    }\n}",
