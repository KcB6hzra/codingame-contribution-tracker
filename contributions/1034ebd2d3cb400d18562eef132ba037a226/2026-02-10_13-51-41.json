      "solution": "import java.util.*;\r\nimport java.io.*;\r\nimport java.math.*;\r\n\r\nclass Solution {\r\n\r\n\t\tpublic static void main(String args[]) {\r\n\t\t\tScanner in = new Scanner(System.in);\r\n\t\t\tint X = in.nextInt();\r\n\t\t\tint Y = in.nextInt();\r\n\t\t\t\t\r\n\t\t\tString num = Integer.toString((int)Math.pow(X, Y));\r\n\t\t\tint totale = 0;\r\n\t\t\tfor (int i = 0 ; i < num.length() ; i++)\r\n\t\t\t{\r\n\t\t\t\ttotale += Integer.parseInt(num.charAt(i)+\"\");\r\n\t\t\t}\r\n\t\t\tSystem.out.println(totale);\r\n\t\t}\r\n}",
